    "dependencies": {
        "@remix-run/express": "^1.6.8",
        "@remix-run/node": "^1.6.8",
        "@remix-run/react": "^1.6.8",
        "compression": "^1.7.4",
        "cross-env": "^7.0.3",
        "dotenv": "^16.0.1",
        "express": "^4.18.1",
        "morgan": "^1.10.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0"
    },
    "devDependencies": {
        "@remix-run/dev": "^1.6.8",
        "@remix-run/eslint-config": "^1.6.8",
        "@types/react": "^18.0.15",
        "@types/react-dom": "^18.0.6",
        "eslint": "^8.20.0",
        "nodemon": "^2.0.19",
        "npm-run-all": "^4.1.5",
        "typescript": "^4.7.4"
    },
    "engines": {
        "node": ">=14"
